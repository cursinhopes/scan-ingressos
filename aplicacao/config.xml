<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.scanneringressoscursinhopes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sacanner Ingressos Cursinho PES</name>
    <description>
        Scanner de ingressos da festa junina do Cursinho PES.
    </description>
    <author email="roger.eliodoro.condras@rogerec.me" href="https://rogerec.me">
        Roger Eliodoro Condras
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
